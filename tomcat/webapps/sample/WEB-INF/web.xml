<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://JAVA.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="2.4">

    <display-name>Hello, World Application</display-name>
    <description>
	This is a simple web application with a source code organization
	based on the recommendations of the Application Developer's Guide.
    </description>

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>mypackage.Hello</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>Hello</display-name>
    	<servlet-name>Hello</servlet-name>
    	<servlet-class>mypackage.Hello</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>CreateNewBlock</display-name>
    	<servlet-name>CreateNewBlock</servlet-name>
    	<servlet-class>mypackage.CreateNewBlock</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>SetStereotype</display-name>
    	<servlet-name>SetStereotype</servlet-name>
    	<servlet-class>mypackage.SetStereotype</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>CreatePorts</display-name>
    	<servlet-name>CreatePorts</servlet-name>
    	<servlet-class>mypackage.CreatePorts</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>ConnectPorts</display-name>
    	<servlet-name>ConnectPorts</servlet-name>
    	<servlet-class>mypackage.ConnectPorts</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>runSCM</display-name>
    	<servlet-name>runSCM</servlet-name>
    	<servlet-class>mypackage.runSCM</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>Hello</servlet-name>
    	<url-pattern>/Hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>CreateNewBlock</servlet-name>
    	<url-pattern>/CreateNewBlock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>SetStereotype</servlet-name>
    	<url-pattern>/SetStereotype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>CreatePorts</servlet-name>
    	<url-pattern>/CreatePorts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>ConnectPorts</servlet-name>
    	<url-pattern>/ConnectPorts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>runSCM</servlet-name>
    	<url-pattern>/runSCM</url-pattern>
    </servlet-mapping>

</web-app>
